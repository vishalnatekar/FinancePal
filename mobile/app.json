{
  "expo": {
    "name": "Personal Finance Hub",
    "slug": "personal-finance-hub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.personalfinancehub.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.personalfinancehub.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "personalfinancehub",
    "plugins": [
      "expo-secure-store",
      [
        "expo-auth-session",
        {
          "schemes": ["personalfinancehub"]
        }
      ]
    ]
  }
}